#: src-en/data/events_ships.xml.append:11987
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_ZOLTAN_LIFERAFT\"]/destroyed/choice/event/choice[1]/text"
msgstr "Отпустить его."

#: src-en/data/events_ships.xml.append:11991
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_ZOLTAN_LIFERAFT\"]/destroyed/choice/event/choice[2]/text"
msgstr "Предложить нанять его за 40 лома."

