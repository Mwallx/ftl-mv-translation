msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_STORM_PIRATE_SUPPLY_FUEL\"]/gotaway/text"
msgstr "Пираты ушли в прыжок."

#: src-en/data/events_ships.xml.append:12013
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_STORM_PIRATE_SUPPLY_FUEL\"]/deadCrew/text"
msgstr ""
