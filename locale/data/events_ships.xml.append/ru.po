#: src-en/data/events_ships.xml.append:8660
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_LEECH_LITTERING\"]/destroyed/choice/text"
msgstr "Собрать мусор."

#: src-en/data/events_ships.xml.append:8668
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_LEECH_LITTERING\"]/deadCrew/choice/text"
msgstr "Собрать мусор."

#: src-en/data/events_ships.xml.append:8680
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_LEECH_TAVERN\"]/destroyed/text"
msgstr "Вы победили лоялистов. Вы разбираете их корабль на металлолом."

#: src-en/data/events_ships.xml.append:8683
