msgstr "Recruter un Engi."

#: src-en/data/events_sector_coalition.xml:1457
msgid ""
"data/events_sector_coalition.xml$//eventList[@name=\"COALITION_JAILER_KILL_SLAVES\"]/event[3]/choice[3]/text"
msgstr "Recruter un Zoltan."

#: src-en/data/events_sector_coalition.xml:1463
