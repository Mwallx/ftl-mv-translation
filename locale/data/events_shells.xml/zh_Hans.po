#: src-en/data/events_shells.xml:92
msgid ""
"data/events_shells.xml$//event[@name=\"SHELL_CLAN_CHOICE\"]/choice[@req=\"cloaking\"]/text"
msgstr "(隐形)隐身，飞走。"

#: src-en/data/events_shells.xml:96
msgid ""
"data/events_shells.xml$//event[@name=\"SHELL_CLAN_CHOICE\"]/choice[@req=\"engines\"]/text"
